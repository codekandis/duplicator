{
	"name": "codekandis/duplicator",
	"description": "`codekandis/duplicator` is a CLI application to identify duplicates in different directory structures and to offer actions to handle them.",
	"keywords": [
		"php",
		"codekandis",
		"duplicates",
		"file",
		"system",
		"directories",
		"actions",
		"cli",
		"application"
	],
	"minimum-stability": "stable",
	"prefer-stable": true,
	"license": "MIT",
	"authors": [
		{
			"name": "Christian Ramelow",
			"email": "info@codekandis.net",
			"homepage": "https://codekandis.net",
			"role": "Developer"
		}
	],
	"support": {
		"email": "info@codekandis.net",
		"issues": "https://github.com/codekandis/duplicator/issues",
		"docs": "https://github.com/codekandis/duplicator/blob/master/README.md",
		"source": "https://github.com/codekandis/duplicator/tree/master"
	},
	"require": {
		"php": "^7.4"
	},
	"require-dev": {
		"ext-xdebug": "*",
		"roave/security-advisories": "dev-master",
		"codekandis/phpunit": "^3"
	},
	"autoload": {
		"psr-4": {
			"CodeKandis\\Duplicator\\": [
				"src/"
			]
		}
	},
	"autoload-dev": {
		"psr-4": {
			"CodeKandis\\Duplicator\\Tests\\": [
				"tests/"
			]
		}
	}
}
